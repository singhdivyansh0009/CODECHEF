//problem : https://www.codechef.com/submit/CCHOCOLATES?tab=statement

#include <iostream>
using namespace std;

int main() {
	int t,x,y,z,total,money_left;
	cin >> t;
	while(t--)
	{
	  cin >> x >> y >> z;
	  total = x*5 + y*10;     // total money chef have
	  money_left = total % z;    //money left by which chef can't be able to buy chocolates
	  if(money_left == 0)
	    cout << total/z << endl;  
	  else
	    cout << (total-money_left)/z << endl;
	}
	return 0;
}
