//problem : https://www.codechef.com/problems/DOLL 

#include <iostream>
using namespace std;

int main() {
   int t,k,n;
	cin >> t;
	while(t--)
	{
      int count=0;
       cin >> n >> k;
	     int x[n]; 
	    for(int i=0 ; i<n ; i++)
	    {
	      cin >> x[i];
	       if(x[i]>k)
	         count++;
	    } 
	   cout << count << endl;
	}
	return 0;
}
